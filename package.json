{
  "name": "hubot-playbook",
  "version": "0.0.0",
  "description": "Conversation branching library for Hubots.",
  "main": "index.coffee",
  "engines": {
    "node": "^4.4.x"
  },
  "scripts": {
    "test": "gulp test",
    "watch": "gulp watch",
    "hubot": "coffee --nodejs '--debug' ./test/scripts/shell.coffee",
    "kill": "kill -9 `ps ax | grep hubot | awk '{print $1}'`",
    "coverage": "gulp test && cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timkinnane/hubot-playbook.git"
  },
  "keywords": [
    "hubot",
    "chatbot",
    "slack",
    "rocketchat",
    "messenger",
    "conversation",
    "dialogue"
  ],
  "author": "Tim Kinnane <tim@nestedcode.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timkinnane/hubot-playbook/issues"
  },
  "homepage": "https://github.com/timkinnane/hubot-playbook#readme",
  "dependencies": {
    "coffee-script": "^1.12.4",
    "conditioner-regex": "0.1.2",

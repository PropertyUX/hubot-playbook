{
  "name": "hubot-playbook",
  "description": "Conversation branching library for Hubots.",
  "main": "lib/",
  "engines": {
    "node": "^4.4.x"
  },
  "scripts": {
    "dev": "run-s compile test",
    "dev:watch": "run-p compile:watch test:watch",
    "build": "run-s compile test docs",
    "precompile": "npm run compile:lint && rimraf lib",
    "compile:lint": "standard src/**/*.js | snazzy",
    "compile": "babel src test/src --out-dir lib",
    "compile:watch": "onchange 'src/**/*.js' -- npm run compile",
    "pretest": "npm run test:lint",
    "test:lint": "coffeelint ./test --quiet",
    "test": "mocha test/modules --bail",
    "test:watch": "onchange 'lib/**/*.js' 'test/**/*.coffee' -d  500 -- npm run test",
    "test:coverage": "mocha test/modules --require ./test/cover.js --reporter-options output=./docs/test/index.html",
    "predocs": "rimraf docs",
    "docs": "run-s docs:jsDocs docs:api",
    "docs:jsDocs": "rimraf md-temp && node docs-assets/jsdoc2md.js",
    "docs:api": "docco ./md-temp/*/*/*.md --template docs-assets/docco.jst --css docs-assets/docco.css --output docs/api",
    "_debug": "mocha --reporter list --no-colors --watch --inspect --debug-brk",
    "_unit:docs": "npm run unit -- --reporter mocha-simple-html-reporter",
    "_coverage": "istanbul report lcovonly text",
    "_coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test",
    "prepush": "npm test",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timkinnane/hubot-playbook.git"
  },
  "keywords": [
    "hubot",
    "chatbot",
    "slack",
    "rocketchat",
    "messenger",
    "conversation",
    "dialogue"
  ],
  "author": "Tim Kinnane <tim@nestedcode.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timkinnane/hubot-playbook/issues"
  },
  "homepage": "https://github.com/timkinnane/hubot-playbook#readme",
  "dependencies": {
    "hooker": "^0.2.3",
    "lodash": "^4.17.4",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node": "^6.1.1",
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "co": "^4.6.0",
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.12.7",
    "coffeelint": "^1.16.0",
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "docco": "^0.7.0",
    "fs-extra": "^4.0.1",
    "handlebars-intl": "^1.1.2",
    "hubot-pretend": "1.1.1",
    "husky": "^0.13.3",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.5.0",
    "mocha-clean": "^1.0.0",
    "mocha-simple-html-reporter": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "validate-commit-msg": "^2.12.0"
  }
}

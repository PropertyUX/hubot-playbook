{
  "name": "hubot-playbook",
  "description": "Conversation branching library for Hubots.",
  "main": "lib/",
  "engines": {
    "node": "^4.4.x"
  },
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs/test",
    "lint:test": "coffeelint ./src ./test",
    "lint:source": "standard src/**/*.js | snazzy",
    "precompile": "rimraf lib",
    "compile": "babel src test/src --source-maps --out-dir lib",
    "compile:watch": "babel src --watch --source-maps --out-dir lib",
    "pretemp": "npm run lint:source && npm run compile",
    "docs:jsDocs": "rimraf md-temp && node docs-assets/jsdoc2md.js",
    "docs:api": "docco ./md-temp/*/*/*.md --template docs-assets/docco.jst --css docs-assets/docco.css --output docs/api",
    "temp": "mocha test/modules --bail --watch",
    "unit": "npm run clean:coverage && mocha",
    "unit:dev": "npm run unit -- --reporter progress --bail",
    "debug": "mocha --reporter list --no-colors --watch --inspect --debug-brk",
    "unit:docs": "npm run unit -- --reporter mocha-simple-html-reporter",
    "coverage": "istanbul report lcovonly text",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "docs": "run-s -s clean:docs unit:docs",
    "dev": "run-s -s lint:dev unit:dev coverage",
    "watch": "onchange 'src/**/*.coffee' 'test/**/*.coffee' -i -- npm run dev",
    "demo": "coffeelint ./demo/scripts && cd demo && npm i && npm test",
    "test": "run-s -s lint unit coverage compile demo",
    "_precommit": "npm test",
    "_prepush": "npm test",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "which-icu": "node -e 'console.dir(\"npm install icu4c-data@\"+process.config.variables.icu_ver_major+process.config.variables.icu_endianness)'",
    "icu-test": "node --icu-data-dir=node_modules/icu4c-data ICU_test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timkinnane/hubot-playbook.git"
  },
  "keywords": [
    "hubot",
    "chatbot",
    "slack",
    "rocketchat",
    "messenger",
    "conversation",
    "dialogue"
  ],
  "author": "Tim Kinnane <tim@nestedcode.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timkinnane/hubot-playbook/issues"
  },
  "homepage": "https://github.com/timkinnane/hubot-playbook#readme",
  "dependencies": {
    "full-icu": "^1.1.3",
    "handlebars": "^4.0.8",
    "handlebars-intl": "^1.1.2",
    "hooker": "^0.2.3",
    "icu4c-data": "^0.58.2",
    "lodash": "^4.17.4",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node": "^6.1.1",
    "chai": "^3.5.0",
    "chai-subset": "^1.5.0",
    "co": "^4.6.0",
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.12.7",
    "coffeelint": "^1.16.0",
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "fs-extra": "^4.0.1",
    "handlebars-intl": "^1.1.2",
    "hubot-pretend": "^1.0.1",
    "husky": "^0.13.3",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.2.0",
    "mocha-clean": "^1.0.0",
    "mocha-simple-html-reporter": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "validate-commit-msg": "^2.12.0"
  }
}
